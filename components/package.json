{
  "name": "@portfolio/components",
  "version": "0.0.1",
  "description": "",
  "main": "./src/index.ts",
  "scripts": {
    "format": "prettier --parser typescript --trailing-comma es5 --single-quote --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "start:dev": "ts-node --project tsconfig.json -r dotenv/config ./src/index.ts",
    "start": "node -r dotenv/config ./lib/src/index.js",
    "clean": "rm -rf lib",
    "build": "tsc --build tsconfig.json",
    "types": "tsc --declaration",
    "compile": "npm run build && npm run types",
    "develop": "npm run compile && npm start:dev",
    "prepublish": "npm run compile",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit"
  },
  "author": "Jon Brennecke",
  "license": "MIT",
  "dependencies": {
    "d3-color": "^1.4.0",
    "lodash": "^4.17.15",
    "polished": "^3.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-controls": "6.4.9",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.4.9",
    "@storybook/addons": "6.4.9",
    "@storybook/react": "6.4.9",
    "@types/d3-color": "^1.2.2",
    "@types/immutable": "3.8.7",
    "@types/lodash": "4.14.149",
    "@types/node": "^12.11.7",
    "@types/styled-components": "4.4.1",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^10.0.1",
    "polished": "^3.5.1",
    "prettier": "1.19.1",
    "tsc": "^2.0.3",
    "typescript": "4.5.4"
  },
  "repository": {
    "url": "https://github.com/jonbrennecke/components",
    "type": "git"
  }
}
